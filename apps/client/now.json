{
    "version": 2,
    "name": "meatup-ayub",
    "alias": "meatup-ayub",
    "builds": [
        { "src": "./index.js", "use": "@now/node-server"}, 
        { "src": "./public/static/**", "use": "@now/static"}
    ],
    "routes": [
        {
            "src": "/static/$1",
            "dest": "/public/static/$1"
        }
    ]
}
